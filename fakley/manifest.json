{
  "manifest_version": 3,
  "name": "Fakely",
  "version": "2.0",
  "description": "Generate live dummy API URLs, export JSON, and deploy to mock services.",
  "permissions": ["clipboardWrite", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png"
  },
  "icons": {
  "16": "icons/icon.png",
  "48": "icons/icon.png",
  "128": "icons/icon.png"
}
}
